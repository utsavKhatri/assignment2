<body>
  <div class="main-content">
    <div class="container px-3 py-5 my-3">
      
      <div class="d-flex flex-row justify-content-between gap-4">
        <h1>Update Category</h1>
        <button
          type="button"
          class="btn btn-secondary"
          onclick="history.back()"
        >
          Back
        </button>
      </div>
        <form
          action="/blogs/update-category/<%= catData._id  %>"
          method="post"
        >
          <div class="mb-3">
            <label for="exampleInputname4" class="form-label"
              >Category Name :</label
            >
            <input
              type="text"
              class="form-control"
              id="exampleInputname4"
              aria-describedby="emailHelp"
              name="categoryName"
              value="<%= catData.categoryName  %>"
              required
            />
          </div>

          <div class="d-flex flex-row justify-content-start gap-4">
            <div>
              <button type="submit" class="btn btn-success">
                Update Category
              </button>
            </div>
            <div>
              <button type="reset" class="btn btn-danger">Cancel</button>
            </div>
          </div>
        </form>
    
    </div>
  </div>
  <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBd3PjUqq81lIOfBPYXrQGWwK5T4ystZjA"></script>

  <% include ../partials/scripts %>
</body>
